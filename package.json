{
  "name": "vite-plugin-asset",
  "type": "module",
  "version": "0.0.1-beta.2",
  "packageManager": "pnpm@9.7.1",
  "description": "Manage your assets of Vite plugins",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wsvaio",
  "homepage": "https://github.com/wsvaio/vite-plugin-asset#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/vite-plugin-asset.git"
  },
  "bugs": {
    "url": "https://github.com/wsvaio/vite-plugin-asset/issues"
  },
  "keywords": [
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./nuxt": {
      "import": {
        "types": "./dist/nuxt.d.mts",
        "default": "./dist/nuxt.mjs"
      },
      "require": {
        "types": "./dist/nuxt.d.cts",
        "default": "./dist/nuxt.cjs"
      }
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rimraf dist && run-s build:* && esno ./scripts/postbuild.ts",
    "build:client": "vite build src/client",
    "build:js": "unbuild",
    "dev": "nr stub && vite src/client",
    "dev:client": "vite build src/client --watch",
    "dev:playground": "nr stub && nr --filter playground dev",
    "stub": "unbuild --stub",
    "tsup": "tsup",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vite": "^3.1.0 || ^4.0.0 || ^5.0.0-0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    }
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "filesize": "^10.1.6",
    "mime": "^4.0.4",
    "picocolors": "^1.0.1",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@iconify/json": "^2.2.239",
    "@nuxt/kit": "^3.12.4",
    "@types/node": "^22.4.1",
    "@unocss/eslint-config": "^0.62.2",
    "@unocss/eslint-plugin": "^0.62.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue-macros/reactivity-transform": "^1.0.3",
    "@vueuse/core": "^11.0.1",
    "@vueuse/router": "^11.0.1",
    "@wsvaio/eslint-config": "^1.0.4",
    "@wsvaio/pinia-plugin-persist": "^1.0.3",
    "@wsvaio/utils": "^1.0.22",
    "bumpp": "^9.5.1",
    "cross-env": "^7.0.3",
    "debug": "^4.3.6",
    "eslint": "^9.9.1",
    "esno": "^4.7.0",
    "highlight.js": "^11.10.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.9",
    "naive-ui": "^2.39.0",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.0",
    "pinia": "^2.2.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "unocss": "^0.62.2",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.7",
    "vite": "^5.4.1",
    "vue": "^3.5.3",
    "vue-router": "^4.4.3",
    "vue-tsc": "^2.0.29"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
